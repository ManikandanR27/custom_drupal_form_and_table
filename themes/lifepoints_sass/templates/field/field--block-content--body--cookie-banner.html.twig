{# empty Twig template #}
{% if label_hidden %}
  {% if multiple %}
	<div>
      {% for item in items %}
        {{ item.content }}
      {% endfor %}
	</div>
  {% else %}
    {% for item in items %}
      <div{{ attributes }}>{{ item.content }}</div>
    {% endfor %}
  {% endif %}
{% else %}
  <div{{ attributes }}>
    <div{{ title_attributes }}>{{ label }}</div>

    {% for item in items %}
      {{ item.content }}
    {% endfor %}

  </div>
{% endif %}